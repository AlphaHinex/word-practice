[{"chinese": "无聊的", "desc": "可通过bore（使厌烦）这个动词来记忆，boring是其形容词形式，表示令人厌烦的，即无聊的。", "english": "boring", "phonetic": "/ˈbɔːrɪŋ/"}, {"chinese": "做运动", "desc": "do表示做，exercise表示运动，直接组合记忆。", "english": "do exercise", "phonetic": ""}, {"chinese": "有时", "desc": "some（一些）和time（时间）组合，联想为一些时间，即有时。", "english": "sometimes", "phonetic": "/ˈsʌmtaɪmz/"}, {"chinese": "简单的", "desc": "ease（轻松、容易）的形容词形式，通过名词记忆形容词。", "english": "easy", "phonetic": "/ˈiːzi/"}, {"chinese": "有趣的", "desc": "interest（兴趣）加上 -ing后缀，有兴趣的就是有趣的。", "english": "interesting", "phonetic": "/ˈɪntrəstɪŋ/"}, {"chinese": "你自己", "desc": "your（你的）和self（自己）组合而成。", "english": "yourself", "phonetic": "/jɔːrˈself/"}, {"chinese": "重要的", "desc": "im - 表示加强意义，port（港口）是重要的地方，加上 -ant形容词后缀。", "english": "important", "phonetic": "/ɪmˈpɔːrtnt/"}, {"chinese": "因为", "desc": "可通过读音记忆这个单词的拼写。", "english": "because", "phonetic": "/bɪˈkɔːz/"}, {"chinese": "健康的", "desc": "health（健康）加上 -y形容词后缀。", "english": "healthy", "phonetic": "/ˈhelθi/"}, {"chinese": "在线的", "desc": "on（在...上）和line（线）组合而成。", "english": "online", "phonetic": "/ˌɑːnˈlaɪn/"}, {"chinese": "爱好", "desc": "直接记忆单词的拼写和含义。", "english": "hobby", "phonetic": "/ˈhɑːbi/"}, {"chinese": "智力游戏", "desc": "puzzle（使迷惑、难题）的复数形式，可通过单数形式记忆。", "english": "puzzles", "phonetic": "/ˈpʌzlz/"}, {"chinese": "电影", "desc": "可通过看电影这个动作来联想记忆单词。", "english": "film", "phonetic": "/fɪlm/"}, {"chinese": "最好的", "desc": "good（好的）的最高级形式，特殊记忆。", "english": "best", "phonetic": "/best/"}, {"chinese": "积极的", "desc": "posit（放置）+ -ive形容词后缀，联想积极的态度放置在心中。", "english": "positive", "phonetic": "/ˈpɑːzətɪv/"}, {"chinese": "消极的", "desc": "neg（否定）+ -ative形容词后缀，否定的态度即消极的。", "english": "negative", "phonetic": "/ˈneɡətɪv/"}, {"chinese": "句子", "desc": "可通过句子的结构来记忆这个单词。", "english": "sentence", "phonetic": "/ˈsentəns/"}, {"chinese": "发生", "desc": "hap（机会、运气）+ -pen后缀，联想事情有机会发生。", "english": "happen", "phonetic": "/ˈhæpən/"}, {"chinese": "简单的", "desc": "sim（相同）+ ple（折叠），联想相同的折叠很简单。", "english": "simple", "phonetic": "/ˈsɪmpl/"}, {"chinese": "基础", "desc": "可通过与basic（基础的）的联系来记忆。", "english": "base", "phonetic": "/beɪs/"}, {"chinese": "辅助的", "desc": "可以通过联想记忆，aux-可看作是'增加'的意思，iliary联想为'力量'，增加力量的即辅助的。", "english": "auxiliary", "phonetic": "/ɔːɡˈzɪliəri/"}]